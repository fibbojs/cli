{
  "name": "fibbo",
  "type": "module",
  "version": "0.0.1",
  "description": "The CLI for the Fibbo game engine",
  "author": "Augustin Mercier 'Gugustinette' <mercier.augustin@proton.me>",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://fibbojs.github.io/fibbo",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fibbojs/cli.git"
  },
  "bugs": "https://github.com/fibbojs/cli/issues",
  "bin": {
    "fibbo": "bin/index.mjs"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "unbuild && npx .",
    "fibbo": "unbuild && npx .",
    "build": "unbuild"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "unbuild": "^2.0.0"
  }
}
